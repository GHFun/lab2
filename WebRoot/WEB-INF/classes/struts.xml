<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE struts PUBLIC "-//Apache Software Foundation//DTD Struts Configuration 2.1//EN" "http://struts.apache.org/dtds/struts-2.1.dtd">
<struts>

	<package name="myaction" namespace=""
		extends="struts-default">
		<action name="login" class="com.Action.Query">
		<result name="success">/index.jsp</result>
		<result name="error">/error.jsp</result>
		</action>
		<action name="insert" class="com.Action.Insert">
		<result name="success">/success.jsp</result>
		<result name="error">/error.jsp</result>
		</action>
		<action name="data" class="com.Action.DatailAction">
			<result name="success">/detail.jsp</result>
			<result name="error">/error.jsp</result>
		</action>
		<action name="delect" class="com.Action.DelectAction">
			<result name="success">/index.jsp</result>
			<result name="error">/error.jsp</result>
		</action>
	</package>
</struts>    
